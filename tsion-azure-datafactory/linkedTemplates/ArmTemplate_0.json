				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "RestApi",
						"fileSystem": "archive"
					}
				},
				"schema": {}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LSadls')]"
